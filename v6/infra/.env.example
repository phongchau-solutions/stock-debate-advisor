        "lifecycle_transition_days": 30
      }
    }
  },
  "prod": {
    "context": {
      "environment": "prod",
      "aws_region": "us-east-1",
      "vpc_cidr": "10.0.0.0/16",
      "database": {
        "instance_type": "t3.small",
        "allocated_storage": 100,
        "backup_retention_days": 30,
        "multi_az": true
      },
      "redis": {
        "instance_type": "t3.small",
        "num_cache_nodes": 3,
        "automatic_failover": true
