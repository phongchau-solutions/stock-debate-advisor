{
  "env": {
    "context": {
      "environment": "dev",
      "aws_region": "us-east-1",
      "vpc_cidr": "10.0.0.0/16",
      "database": {
        "instance_type": "t3.micro",
        "allocated_storage": 20,
        "backup_retention_days": 7,
        "multi_az": false
      },
      "redis": {
        "instance_type": "t3.micro",
        "num_cache_nodes": 1,
