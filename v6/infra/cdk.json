{
  "app": "npx ts-node --prefer-ts-exts src/bin/stock-debate-advisor.ts",
  "watch": {
    "include": ["**"],
    "exclude": ["README.md", "cdk.out", "node_modules"]
  },
  "context": {
    "environment": "dev",
    "vpc_cidr": "10.0.0.0/16",
    "enable_nat": true,
    "enable_database": true,
    "enable_cache": true,
    "database_instance_type": "t3.micro",
    "cache_instance_type": "cache.t3.micro"
  }
}
