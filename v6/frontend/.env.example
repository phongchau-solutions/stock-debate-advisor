// Example environment variables for frontend

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_DATA_SERVICE_URL=http://localhost:8003
VITE_AGENTIC_SERVICE_URL=http://localhost:8001
VITE_ANALYSIS_SERVICE_URL=http://localhost:8002

# Environment
VITE_ENV=development

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false

# Timeouts (in ms)
VITE_REQUEST_TIMEOUT=30000
VITE_DEBATE_TIMEOUT=60000

# UI Configuration
VITE_ENABLE_MATERIAL_DESIGN_3=true
VITE_THEME_MODE=light
